{
  "components": {
    "schemas": {
       "RootMsgSchema": 
          {
          "type": "object",
          "properties": {
            "ArrayElement": {
              "$ref":"#/components/schemas/tns_ArrayType"
            }
          },
          "required": ["ArrayElement"]
        },
      "tns_ArrayType": 
          {
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "PrivilegeIntArray": {
                  "oneOf": [
                    {
                        "type": "number"
                    },
                    {
                      "type": "array",
                      "minItems": 1,
                      "maxItems": 2,
                      "items": {
                        "type": "number"
                      }
                    }
                  ]
                }
              }
            },
            {
              "type": "object",
              "properties": {
                "PrivilegeStrArray": {
                  "oneOf": [
                    {
                        "type": "string"
                    },
                    {
                      "type": "array",
                      "minItems": 1,
                      "maxItems": 2,
                      "items": {
                        "type": "string"
                      }
                    }
                  ]
                }
              }
            },
            {
              "type": "object",
              "properties": {
                "NumberWithRangeArray": {
                  "oneOf": [
                    {
                        "$ref":"#/components/schemas/tns_NumberWithRange"
                    },
                    {
                      "type": "array",
                      "minItems": 1,
                      "maxItems": 2,
                      "items": {
                        "$ref":"#/components/schemas/tns_NumberWithRange"
                      }
                    }
                  ]
                }
              }
            },
            {
              "type": "object",
              "properties": {
                "NumberWithDigitsArray": {
                  "oneOf": [
                    {
                        "$ref":"#/components/schemas/tns_NumberWithDigits"
                    },
                    {
                      "type": "array",
                      "minItems": 1,
                      "maxItems": 2,
                      "items": {
                        "$ref":"#/components/schemas/tns_NumberWithDigits"
                      }
                    }
                  ]
                }
              }
            },
            {
              "type": "object",
              "properties": {
                "StringArray": {
                  "oneOf": [
                    {
                        "$ref":"#/components/schemas/tns_SimpleText"
                    },
                    {
                      "type": "array",
                      "minItems": 1,
                      "maxItems": 2,
                      "items": {
                        "$ref":"#/components/schemas/tns_SimpleText"
                      }
                    }
                  ]
                }
              }
            },
            {
              "type": "object",
              "properties": {
                "BooleanArray": {
                  "oneOf": [
                    {
                        "$ref":"#/components/schemas/tns_BooleanType"
                    },
                    {
                      "type": "array",
                      "minItems": 1,
                      "maxItems": 2,
                      "items": {
                        "$ref":"#/components/schemas/tns_BooleanType"
                      }
                    }
                  ]
                }
              }
            }
          ]
        },
      "tns_NumberWithRange": {
        "type": "number",
        "minimum": 10,
        "maximum": 100
      },
      "tns_NumberWithDigits": {
        "type": "string",
        "pattern": "^\\d{0,18}(\\.\\d{0,5})?$"
      },
      "tns_SimpleText": {
        "type": "string"
      },
      "tns_BooleanType": {
        "type": "boolean"
      }
    }
  }
}